{
  "realm": "sts-realm",
  "enabled": true,
  "displayName": "STS Local Realm",
  "clients": [
    {
      "clientId": "sts-api",
      "name": "STS Minimal API",
      "rootUrl": "http://localhost:5239",
      "redirectUris": [
        "http://localhost:5239/*",
        "http://localhost:5239/scalar*"
      ],
      "webOrigins": ["+"],
      "publicClient": true,
      "standardFlowEnabled": true,
      "implicitFlowEnabled": false,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": false,
      "protocol": "openid-connect",
      "attributes": {
        "pkce.code.challenge.method": "S256"
      }
    }
  ],
  "users": [
    {
      "username": "api-user",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Api",
      "lastName": "User",
      "email": "api-user@example.com",
      "credentials": [
        {
          "type": "password",
          "value": "pwd",
          "temporary": false
        }
      ],
      "realmRoles": ["offline_access", "default-roles-sts-realm"],
      "clientRoles": {}
    }
  ]
}
